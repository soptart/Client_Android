<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorMainBlack"
        android:orientation="vertical"
        tools:context=".JoinActivity">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="87dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/login_logo"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="22dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Join us"
                android:textSize="48sp"
                android:textColor="#ffffff"
                android:textStyle="bold"/>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/ll_join_first"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="visible"
            android:layout_marginLeft="31dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="49dp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="64dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="20dp"/>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:text=" 이름"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/notosanscjkkr_medium"/>
                <EditText
                    android:layout_marginLeft="5dp"
                    android:id="@+id/et_join_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textAlignment="viewEnd"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:textColorHint="#c6c6c6"
                    android:backgroundTint="#c6c6c6"
                    android:imeOptions="actionDone"
                    android:layout_alignParentBottom="true"
                    android:fontFamily="@font/notosanscjkkr_demilight"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="64dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="20dp"/>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:text=" 이메일"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/notosanscjkkr_medium"/>
                <EditText
                    android:layout_marginLeft="5dp"
                    android:id="@+id/et_join_email"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:hint="ex.yic8966@artoo.com"
                    android:textColorHint="#ababab"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:textAlignment="viewEnd"
                    android:backgroundTint="#c6c6c6"
                    android:imeOptions="actionDone"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="64dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="20dp"/>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:text=" 비밀번호"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/notosanscjkkr_medium"/>
                <EditText
                    android:layout_marginLeft="5dp"
                    android:id="@+id/et_join_password"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textAlignment="viewEnd"
                    android:textSize="16sp"
                    android:hint="8자리 이상"
                    android:textColorHint="#ababab"
                    android:textColor="#c6c6c6"
                    android:backgroundTint="#c6c6c6"
                    android:imeOptions="actionDone"
                    android:inputType="textPassword"
                    android:layout_alignParentBottom="true"
                    android:fontFamily="@font/notosanscjkkr_demilight"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginBottom="10dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="11dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 필수 정보"
                    android:textSize="15sp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="#ff6f61"/>
                <ImageButton
                    android:id="@+id/btn_join_next1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="36dp"
                    android:background="@android:color/transparent"
                    android:clickable="true"
                    android:src="@drawable/join_next"/>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_join_second"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="gone"
            android:layout_marginLeft="31dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="49dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="64dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="20dp"/>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:text=" 연락처"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:gravity="center_vertical"
                    android:layout_alignParentBottom="true"
                    android:fontFamily="@font/notosanscjkkr_medium"/>
                <EditText
                    android:layout_marginLeft="5dp"
                    android:id="@+id/et_join_phone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:hint=" '-' 없이 입력"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textAlignment="viewEnd"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:textColorHint="#ababab"
                    android:backgroundTint="#c6c6c6"
                    android:imeOptions="actionDone" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="64dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="20dp"/>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:text=" 주소"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/notosanscjkkr_medium"/>
                <EditText
                    android:layout_marginLeft="5dp"
                    android:id="@+id/et_join_location"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:hint=" ex. 서울시 광진구"
                    android:textAlignment="viewEnd"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:textColorHint="#ababab"
                    android:backgroundTint="#c6c6c6"
                    android:imeOptions="actionDone"
                    android:layout_alignParentBottom="true"
                    android:fontFamily="@font/notosanscjkkr_demilight" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="64dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="20dp"
                    android:visibility="invisible"/>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:text=" 대학교"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/notosanscjkkr_medium"/>
                <EditText
                    android:layout_marginLeft="5dp"
                    android:id="@+id/et_join_university"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="대학교"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textAlignment="viewEnd"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:backgroundTint="#c6c6c6"
                    android:imeOptions="actionDone"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginBottom="10dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="11dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 필수 정보"
                    android:textSize="15sp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="#ff6f61"/>
                <ImageButton
                    android:id="@+id/btn_join_next2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="36dp"
                    android:background="@android:color/transparent"
                    android:clickable="true"
                    android:src="@drawable/join_next"/>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_join_third"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="gone"
            android:layout_marginLeft="31dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="49dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="64dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="20dp"/>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:text=" 은행"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:gravity="center_vertical"
                    android:layout_alignParentBottom="true"
                    android:fontFamily="@font/notosanscjkkr_medium"/>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:id="@+id/tv_join_bank"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_toLeftOf="@id/btn_join_bank"
                    android:layout_marginRight="16dp"
                    android:gravity="center_vertical"
                    android:layout_centerVertical="true"
                    android:text=""
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:layout_alignParentBottom="true"/>
                <EditText
                    android:layout_marginLeft="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:backgroundTint="#c6c6c6"
                    android:imeOptions="actionDone"
                    android:clickable="false"
                    android:enabled="false"
                    android:layout_alignParentBottom="true"/>
                <ImageButton
                    android:id="@+id/btn_join_bank"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:src="@drawable/arrow_down_white" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="64dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="20dp"/>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:text=" 계좌번호"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/notosanscjkkr_medium"/>
                <EditText
                    android:layout_marginLeft="5dp"
                    android:id="@+id/et_join_account"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:hint=" '-' 없이 입력"
                    android:textAlignment="viewEnd"
                    android:textSize="16sp"
                    android:textColor="#c6c6c6"
                    android:textColorHint="#ababab"
                    android:backgroundTint="#c6c6c6"
                    android:imeOptions="actionDone"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:layout_alignParentBottom="true" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginBottom="10dp">
                <View
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/dialog_oval"
                    android:layout_marginTop="11dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 필수 정보"
                    android:textSize="15sp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="#ff6f61"/>
                <ImageButton
                    android:id="@+id/btn_join_success"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="90sp"
                    android:layout_centerInParent="true"
                    android:background="@android:color/transparent"
                    android:clickable="true"
                    android:src="@drawable/join_icon"
                    android:layout_alignParentTop="true"/>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

    <!-- select bank layout -->
    <LinearLayout
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/ll_join_bank"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:background="#CC000000"
        android:orientation="vertical"
        xmlns:android="http://schemas.android.com/apk/res/android">

        <RelativeLayout
            android:id="@+id/rl_join_bank_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right">
            <ImageView
                android:id="@+id/btn_join_bank_close"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_marginTop="38dp"
                android:layout_marginRight="15dp"
                android:background="@android:color/transparent"
                android:src="@drawable/close_white"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="28dp"
                android:text="은행"
                android:textColor="#ffffff"
                android:textSize="28sp"
                android:fontFamily="@font/notosanscjkkr_regular"
                android:background="@android:color/transparent"/>
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rl_join_bank_top"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="80dp"
            android:layout_marginTop="40dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="1">
                <Button
                    android:id="@+id/btn_join_bank_kb"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="KB국민"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center"/>

                <Button
                    android:id="@+id/btn_join_bank_keb"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="KEB하나"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center"/>

                <Button
                    android:id="@+id/btn_join_bank_shin"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="신한"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="1">

                <Button
                    android:id="@+id/btn_join_bank_wr"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="우리"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center" />

                <Button
                    android:id="@+id/btn_join_bank_nh"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="NH농협"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center" />

                <Button
                    android:id="@+id/btn_join_bank_ibk"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="IBK기업"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="1">

                <Button
                    android:id="@+id/btn_join_bank_kdb"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="KDB산업"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center"/>

                <Button
                    android:id="@+id/btn_join_bank_ct"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="한국씨티"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center"/>

                <Button
                    android:id="@+id/btn_join_bank_sc"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="SC제일"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="1">

                <Button
                    android:id="@+id/btn_join_bank_bmk"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="BNK부산"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center" />

                <Button
                    android:id="@+id/btn_join_bank_dgb"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="DGB대구"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center"/>

                <Button
                    android:id="@+id/btn_join_bank_bnk"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:background="@drawable/bank_border"
                    android:text="BNK경남"
                    android:textSize="16sp"
                    android:fontFamily="@font/notosanscjkkr_demilight"
                    android:textColor="@color/colorBorder"
                    android:textAlignment="center"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginLeft="8dp"
