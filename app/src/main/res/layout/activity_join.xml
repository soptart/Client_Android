                        android:fontFamily="@font/notosanscjkkr_regular"
                        android:textAlignment="viewEnd"
                        android:textSize="16sp"
                        android:textColor="#ffffff"
                        android:backgroundTint="#c6c6c6"
                        android:imeOptions="actionDone"
                        android:layout_alignParentBottom="true"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:text="대학교"
                        android:layout_alignParentRight="true"
                        android:textColor="#dbdbdb"
                        android:textSize="16sp"
                        android:fontFamily="@font/notosanscjkkr_medium"
                        android:includeFontPadding="false"/>
            </RelativeLayout>

            <TextView
                android:id="@+id/join_check_univ"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="유효한 연락처가 아닙니다!"
                android:textSize="12sp"
                android:textColor="#ff6f61"
                android:layout_gravity="end"
                android:layout_marginBottom="7dp"
                android:visibility="invisible"/>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginBottom="10dp">
                <View
                        android:layout_width="4dp"
                        android:layout_height="4dp"
                        android:background="@drawable/dialog_oval"
                        android:layout_marginTop="11dp"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 필수 정보"
                        android:includeFontPadding="false"
                        android:textSize="15sp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/notosanscjkkr_demilight"
                        android:textColor="#ff6f61"/>
                <ImageButton
                        android:id="@+id/btn_join_next2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="88dp"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:src="@drawable/join_next"/>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
