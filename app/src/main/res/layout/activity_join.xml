                        android:layout_height="match_parent"
                        android:textAlignment="viewEnd"
                        android:textSize="16sp"
                        android:hint="8자리 이상"
                        android:includeFontPadding="false"
                        android:textColorHint="#ababab"
                        android:textColor="#ffffff"
